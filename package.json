{
    "name": "esifycss-webpack-plugin",
    "version": "0.1.3",
    "license": "Apache-2.0",
    "author": {
        "name": "Kei Ito",
        "email": "kei.itof@gmail.com",
        "url": "https://github.com/kei-ito"
    },
    "homepage": "https://github.com/kei-ito/esifycss-webpack-plugin",
    "repository": "https://github.com/kei-ito/esifycss-webpack-plugin",
    "engines": {
        "node": ">=12"
    },
    "main": "./lib/cjs.js",
    "files": [
        "lib"
    ],
    "scripts": {
        "build": "tsc --project tsconfig.build.json",
        "test": "run-s test:*",
        "test:type": "tsc --noEmit",
        "test:unit": "ava",
        "lint": "eslint --ext .ts src",
        "version": "run-s version:changelog version:add",
        "version:changelog": "npx @nlib/changelog --output CHANGELOG.md",
        "version:add": "git add ."
    },
    "devDependencies": {
        "@nlib/eslint-config": "3.17.25",
        "@nlib/githooks": "0.1.0",
        "@typescript-eslint/eslint-plugin": "4.33.0",
        "@typescript-eslint/parser": "4.33.0",
        "ava": "3.15.0",
        "eslint": "7.32.0",
        "lint-staged": "11.2.0",
        "npm-run-all": "4.1.5",
        "ts-node": "10.2.1",
        "typescript": "4.4.3",
        "webpack": "5.54.0"
    },
    "eslintConfig": {
        "extends": [
            "@nlib/eslint-config"
        ],
        "env": {
            "node": true
        },
        "rules": {
            "@nlib/no-globals": "off"
        }
    },
    "ava": {
        "extensions": [
            "ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "files": [
            "test/**/*.test.ts"
        ]
    },
    "lint-staged": {
        "*.ts": [
            "eslint"
        ],
        "*.js": [
            "eslint"
        ]
    },
    "renovate": {
        "extends": [
            "github>nlibjs/renovate-config"
        ]
    }
}
